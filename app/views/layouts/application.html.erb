<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= render 'layouts/rails_default' %>
    <%= render 'layouts/shim' %>
  </head>


  <body class="mdl-layout mdl-js-layout">
    <%= render 'layouts/header' %>

    <section class="mdl-layout__content">
      <div class="page-content">
        <%= yield %>
      </div>
    </section>

    
    <%= render 'layouts/footer' %>
  </body>
</html>
